msgid "Edit host"
msgstr "Redigér vært"

#: gtkhostsetup.glade:934 gtkhostsetup.desktop.in.h:1
#: gtkhostsetup-kde.desktop.in.h:1
msgid "Configure the system hostnames"
msgstr "Konfigurér systemets værtsnavne"

#: gtkhostsetup.glade:1613
msgid "translator-credits"
msgstr "Kris Thomsen <lakristho~at~gmail~dot~com>"

#: gtkhostsetup:60
msgid "The IP address cannot be empty."
msgstr "IP-adressen må ikke være tom."

#: gtkhostsetup:63
msgid "The hostname cannot be empty."
msgstr "Værtsnavnet må ikke være tom."

#: gtkhostsetup:66
msgid "The hostname cannot start with a hyphen."
msgstr "Værtsnavnet må ikke starte med en bindestreg."

#: gtkhostsetup:69
msgid "The hostname cannot end with a hyphen."
msgstr "Værtsnavnet må ikke slutte med en bindestreg."

#: gtkhostsetup:72
msgid "The domain cannot start with a hyphen."
msgstr "Domænet må ikke starte med en bindestreg."

#: gtkhostsetup:75
msgid "The domain cannot end with a hyphen."
msgstr "Domænet må ikke slutte med en bindestreg."

#: gtkhostsetup:80
msgid "Not a valid IP address."
msgstr "Ikke en gyldig IP-adresse."

#: gtkhostsetup:85
msgid "The hostname can consist solely of latin letters, numbers and hyphens."
msgstr "Værtsnavnet må kun indeholdet latin-bogstaver, tal og bindestreger."

#: gtkhostsetup:90
msgid ""
"The domain can consist solely of latin letters, numbers, hyphens and dots."
msgstr "Domænet må kun indeholdet latin-bogstaver, tal og bindestreger."

#: gtkhostsetup:284
msgid "IP address"
msgstr "IP-adresse"

#: gtkhostsetup:285
msgid "Hostname"
msgstr "Værtsnavn"

#: gtkhostsetup:286
msgid "Domain"
msgstr "Domæne"
