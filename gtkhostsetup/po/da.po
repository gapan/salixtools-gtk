#: gtkhostsetup-kde.desktop.in.h:2
msgid "Hostnames"
msgstr "Værtsnavne"

#: gtkhostsetup.glade:44
msgid "<b>This computer:</b>"
msgstr "<b>Denne computer:</b>"

#: gtkhostsetup.glade:88
#: gtkhostsetup.glade:364
#: gtkhostsetup.glade:596
msgid "Hostname:"
msgstr "Værtsnavn:"

#: gtkhostsetup.glade:95
#: gtkhostsetup.glade:375
#: gtkhostsetup.glade:607
msgid "Domain:"
msgstr "Domæne:"

#: gtkhostsetup.glade:124
msgid "<b>Other hosts:</b>"
msgstr "<b>Andre værter:</b>"

#: gtkhostsetup.glade:329
msgid "Add host"
msgstr "Tilføj vært"

#: gtkhostsetup.glade:357
#: gtkhostsetup.glade:589
msgid "IP address:"
msgstr "IP-adresse:"

#: gtkhostsetup.glade:474
#: gtkhostsetup.glade:706
#: gtkhostsetup.glade:793
msgid "Error"
msgstr "Fejl"

#: gtkhostsetup.glade:561
msgid "Edit host"
msgstr "Redigér vært"

#: gtkhostsetup:60
msgid "The IP address cannot be empty."
msgstr "IP-adressen må ikke være tom."

#: gtkhostsetup:63
msgid "The hostname cannot be empty."
msgstr "Værtsnavnet må ikke være tom."

#: gtkhostsetup:66
msgid "The hostname cannot start with a hyphen."
msgstr "Værtsnavnet må ikke starte med en bindestreg."

#: gtkhostsetup:69
msgid "The hostname cannot end with a hyphen."
msgstr "Værtsnavnet må ikke slutte med en bindestreg."

#: gtkhostsetup:72
msgid "The domain cannot start with a hyphen."
msgstr "Domænet må ikke starte med en bindestreg."

#: gtkhostsetup:75
msgid "The domain cannot end with a hyphen."
msgstr "Domænet må ikke slutte med en bindestreg."

#: gtkhostsetup:80
msgid "Not a valid IP address."
msgstr "Ikke en gyldig IP-adresse."

#: gtkhostsetup:85
msgid "The hostname can consist solely of latin letters, numbers and hyphens."
msgstr "Værtsnavnet må kun indeholdet latin-bogstaver, tal og bindestreger."

#: gtkhostsetup:90
msgid "The domain can consist solely of latin letters, numbers, hyphens and dots."
msgstr "Domænet må kun indeholdet latin-bogstaver, tal og bindestreger."

#: gtkhostsetup:274
msgid "IP address"
msgstr "IP-adresse"

#: gtkhostsetup:275
msgid "Hostname"
msgstr "Værtsnavn"

#: gtkhostsetup:276
msgid "Domain"
msgstr "Domæne"

#: gtkhostsetup.desktop.in.h:1
#: gtkhostsetup-kde.desktop.in.h:1
msgid "Configure the system hostnames"
msgstr "Konfigurér systemets værtsnavne"

