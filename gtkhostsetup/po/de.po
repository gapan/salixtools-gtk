msgid "<b>Other hosts:</b>"
msgstr "<b>Andere Hosts:</b>"

#: gtkhostsetup.glade:329
msgid "Add host"
msgstr "Host hinzufügen"

#: gtkhostsetup.glade:357 gtkhostsetup.glade:589
msgid "IP address:"
msgstr "IP-Adresse:"

#: gtkhostsetup.glade:474 gtkhostsetup.glade:706 gtkhostsetup.glade:793
msgid "Error"
msgstr "Fehler"

#: gtkhostsetup.glade:561
