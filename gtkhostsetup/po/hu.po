msgid "Hostnames"
msgstr "Számítógépek nevei"

#: gtkhostsetup.glade:44
msgid "<b>This computer:</b>"
msgstr "<b>Ez a számítógép:</b>"

#: gtkhostsetup.glade:88
#: gtkhostsetup.glade:364
#: gtkhostsetup.glade:596
msgid "Hostname:"
msgstr "Gépnév:"

#: gtkhostsetup.glade:95
