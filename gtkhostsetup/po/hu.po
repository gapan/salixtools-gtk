#: gtkhostsetup.glade:44
msgid "<b>This computer:</b>"
msgstr "<b>Ez a számítógép:</b>"

#: gtkhostsetup.glade:88 gtkhostsetup.glade:404 gtkhostsetup.glade:637
msgid "Hostname:"
msgstr "Gépnév:"

#: gtkhostsetup.glade:95 gtkhostsetup.glade:415 gtkhostsetup.glade:648
msgid "Domain:"
msgstr "Tartomány:"

#: gtkhostsetup.glade:124
msgid "<b>Other hosts:</b>"
msgstr "<b>Egyéb gépek:</b>"

#: gtkhostsetup.glade:369
msgid "Add host"
msgstr "Gép hozzáadása"

#: gtkhostsetup.glade:397 gtkhostsetup.glade:630
msgid "IP address:"
msgstr "IP-cím:"

#: gtkhostsetup.glade:514 gtkhostsetup.glade:747 gtkhostsetup.glade:835
msgid "Error"
msgstr "Hiba"

#: gtkhostsetup.glade:602
