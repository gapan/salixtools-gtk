#: gtkhostsetup.glade:607
msgid "Domain:"
msgstr "Domain:"

#: gtkhostsetup.glade:124
msgid "<b>Other hosts:</b>"
msgstr "<b>Egyéb gépek:</b>"

#: gtkhostsetup.glade:329
msgid "Add host"
msgstr "Gép hozzáadása"

#: gtkhostsetup.glade:357
#: gtkhostsetup.glade:589
msgid "IP address:"
msgstr "IP cím:"

#: gtkhostsetup.glade:474
#: gtkhostsetup.glade:706
#: gtkhostsetup.glade:793
msgid "Error"
msgstr "Hiba"

#: gtkhostsetup.glade:561
msgid "Edit host"
msgstr "Gép szerkesztése"

#: gtkhostsetup:60
msgid "The IP address cannot be empty."
msgstr "Az IP cím nem lehet üres."

#: gtkhostsetup:63
msgid "The hostname cannot be empty."
msgstr "A gép év nem lehet üres."

#: gtkhostsetup:66
msgid "The hostname cannot start with a hyphen."
msgstr "A gépnév nem kezdődhet kötőjellel."

#: gtkhostsetup:69
msgid "The hostname cannot end with a hyphen."
msgstr "A gépnév nem végződhet kötőjellel."

#: gtkhostsetup:72
msgid "The domain cannot start with a hyphen."
msgstr "A domain név nem kezdődhet kötőjellel."

#: gtkhostsetup:75
msgid "The domain cannot end with a hyphen."
msgstr "A domain név nem végződhet kötőjellel."

#: gtkhostsetup:80
msgid "Not a valid IP address."
msgstr "Nem valós az IP cím."

#: gtkhostsetup:85
msgid "The hostname can consist solely of latin letters, numbers and hyphens."
msgstr "A gépnév kizárólag latin betűkből, számokból és kötőjelből állhat."

#: gtkhostsetup:90
msgid "The domain can consist solely of latin letters, numbers, hyphens and dots."
msgstr "A domain név kizárólag latin betűkből, számokból és kötőjelből állhat."

#: gtkhostsetup:274
msgid "IP address"
msgstr "IP cím"

#: gtkhostsetup:275
msgid "Hostname"
msgstr "Gépnév"

#: gtkhostsetup:276
msgid "Domain"
msgstr "Domain"

#: gtkhostsetup.desktop.in.h:1
