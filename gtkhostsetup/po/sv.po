msgid "Add host"
msgstr "Lägg till värd"

#: gtkhostsetup.glade:397 gtkhostsetup.glade:630
msgid "IP address:"
msgstr "IP adress:"

#: gtkhostsetup.glade:514 gtkhostsetup.glade:747 gtkhostsetup.glade:835
msgid "Error"
msgstr "Fel"

#: gtkhostsetup.glade:602
msgid "Edit host"
msgstr "Redigera värd"

#: gtkhostsetup.glade:934 gtkhostsetup.desktop.in.h:1
#: gtkhostsetup-kde.desktop.in.h:1
msgid "Configure the system hostnames"
msgstr "Konfigurera systemets värdnamn"

#: gtkhostsetup.glade:1613
msgid "translator-credits"
msgstr "Ingemar Karlsson <po@ingk.se>"

#: gtkhostsetup:60
msgid "The IP address cannot be empty."
msgstr "IP adressen kan inte vara tom."

#: gtkhostsetup:63
msgid "The hostname cannot be empty."
msgstr "Värdnamn kan inte vara tom."

#: gtkhostsetup:66
msgid "The hostname cannot start with a hyphen."
msgstr "Värdnamn kan inte börja med bindestreck."

#: gtkhostsetup:69
msgid "The hostname cannot end with a hyphen."
msgstr "Värdnamn kan inte sluta med bindestreck."

#: gtkhostsetup:72
msgid "The domain cannot start with a hyphen."
msgstr "Domännamn kan inte börja med bindestreck."

#: gtkhostsetup:75
msgid "The domain cannot end with a hyphen."
msgstr "Domännamn kan inte sluta med bindestreck."

#: gtkhostsetup:80
msgid "Not a valid IP address."
msgstr "Inte en giltig IP adress."

#: gtkhostsetup:85
msgid "The hostname can consist solely of latin letters, numbers and hyphens."
msgstr "Värdnamn kan bara innehålla bokstäver, siffror och bindestreck."

#: gtkhostsetup:90
msgid ""
"The domain can consist solely of latin letters, numbers, hyphens and dots."
msgstr "Domännamn kan bara innehålla bokstäver, siffror och bindestreck."

#: gtkhostsetup:284
msgid "IP address"
msgstr "IP adress"

#: gtkhostsetup:285
msgid "Hostname"
msgstr "Värdnamn"

