msgid "<b>This computer:</b>"
msgstr "<b>Este computador:</b>"

#: gtkhostsetup.glade:88 gtkhostsetup.glade:404 gtkhostsetup.glade:637
msgid "Hostname:"
msgstr "Nome de anfitrião"

#: gtkhostsetup.glade:95 gtkhostsetup.glade:415 gtkhostsetup.glade:648
msgid "Domain:"
msgstr "Domínio:"

