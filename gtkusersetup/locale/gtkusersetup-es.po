#: ../gtkusersetup:209
msgid "Password is too short. It has to be at least 5 characters long."
msgstr "La contraseña es demasiado corta. Tiene que tener al menos 5 caracteres."

#: ../gtkusersetup:212
msgid "Password cannot include the backslash character."
msgstr "La contraseña no puede incluir un caracter de barra invertida."

#: ../gtkusersetup:218
msgid "Passwords don't match"
msgstr "Las contraseñas no coinciden"

#: ../gtkusersetup:227
msgid "Username cannot be empty."
msgstr "El nombre de usuario no puede estar vacío."

#: ../gtkusersetup:230
msgid "Username cannot exceed 31 characters"
msgstr "El nombre de usuario no puede exceder los 31 caracteres"

#: ../gtkusersetup:233
msgid "Username cannot include the space character."
msgstr "El nombre de usuario no puede incluir espacios."

#: ../gtkusersetup:236
msgid "Username cannot start with a number."
msgstr "El nombre de usuario no puede comenzar con un número."

#: ../gtkusersetup:239
msgid "Username cannot start with an underscore."
msgstr "El nombre de usuario no puede comenzar con un caracter de subrayado."

#: ../gtkusersetup:242
msgid "Username cannot start with a dash."
msgstr "El nombre de usuario no puede comenzar con un guión."

#: ../gtkusersetup:247
msgid "Username can consist solely of latin lower case letters, numbers, dashes and underscores."
msgstr "El nombre de usuario solamente puede formarse con letras minúsculas, números, guiones y caracteres de subrayado."

#: ../gtkusersetup:253
msgid "Username \"{0}\" already exists."
msgstr "El nombre de usuario \"{0}\" ya existe."

#: ../gtkusersetup:263
msgid "Group name cannot be empty."
msgstr "El nombre del grupo no puede estar vacío."

#: ../gtkusersetup:266
msgid "Group name cannot exceed 31 characters"
msgstr "El nombre del grupo no puede exceder los 31 caracteres"

#: ../gtkusersetup:269
msgid "Group name cannot include the space character."
msgstr "El nombre del grupo no puede incluir espacios."

#: ../gtkusersetup:272
msgid "Group name cannot start with a number."
msgstr "El nombre del grupo no puede comenzar con un número."

#: ../gtkusersetup:275
msgid "Group name cannot start with an underscore."
msgstr "El nombre del grupo no puede comenzar con un caracter de subrayado."

#: ../gtkusersetup:278
msgid "Group name cannot start with a dash."
msgstr "El nombre del grupo no puede comenzar con un guión."

#: ../gtkusersetup:283
msgid "Group name can consist solely of latin lower case letters, numbers, dashes and underscores."
msgstr "El nombre de grupo solamente puede formarse con letras minúsculas, números, guiones y caracteres de subrayado."

#: ../gtkusersetup:289
msgid "Group name \"{0}\" already exists."
msgstr "Ya existe el nombre de grupo \"{0}\"."

#: ../gtkusersetup:299
msgid "Shell does not exist."
msgstr "El shell no existe."

#: ../gtkusersetup:309
msgid "UID {0} is assigned to another user."
msgstr "El UID {0} está asignado a otro usuario."

#: ../gtkusersetup:319
msgid "GID {0} is assigned to another group."
msgstr "El GID {0} está asignado a otro grupo."

#: ../gtkusersetup:359
msgid "User account properties for \"{0}\""
msgstr "Propiedades de la cuenta de usuario para \"{0}\""

#: ../gtkusersetup:366
msgid "<b>Set new password</b>"
msgstr "<b>Establecer una contraseña nueva</b>"

#: ../gtkusersetup:421
msgid "Add new user"
msgstr "Agregar un usuario nuevo"

#: ../gtkusersetup:428
msgid "<b>Set password</b>"
msgstr "<b>Establecer contraseña</b>"

#: ../gtkusersetup:477
msgid "Delete user {0}?"
msgstr "¿Eliminar el usuario {0}?"

#: ../gtkusersetup:478
msgid "You are about to remove user account \"{0}\" from your system. Do you really want to continue?"
msgstr "Está a punto de eliminar de su sistema la cuenta del usuario \"{0}\". ¿Realmente quiere continuar?"

#: ../gtkusersetup:479
msgid "Completely remove user's home directory ({0}) with all its contents"
msgstr "Se borró completamente el directorio home del usuario ({0}) junto con todo su contenido"

#: ../gtkusersetup:724
msgid "Group \"{0}\" properties"
msgstr "Propiedades del grupo \"{0}\""

#: ../gtkusersetup:743
msgid "Add new group"
msgstr "Agregar un grupo nuevo"

#: ../gtkusersetup:768
msgid "You are about to delete the \"{0}\" group from your system. Are you sure you want to continue?"
msgstr "Está a punto de borrar de su sistema el grupo \"{0}\". ¿Está seguro que quiere continuar?"

#: ../gtkusersetup:771
msgid "Group \"{0}\" cannot be deleted as it is the main group for user \"{1}\". If you want to delete group \"{0}\", you have to set the main group for user \"{1}\" to another group first."
msgstr "No se puede borrar el grupo \"{0}\" porque es el grupo principal del usuario \"{1}\". Si quiere borrar el grupo \"{0}\", tiene que configurar primero el grupo principal para el usuario \"{1}\" a otro grupo."

