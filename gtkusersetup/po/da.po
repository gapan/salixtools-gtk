#: gtkusersetup.glade:1358
msgid "<b>Basic settings</b>"
msgstr "<b>Grundlæggende indstillinger</b>"

#: gtkusersetup.glade:1378
msgid "Group name:"
msgstr "Gruppenavn:"

#: gtkusersetup.glade:1388
msgid "Group ID:"
msgstr "Gruppe-id:"

