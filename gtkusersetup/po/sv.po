msgstr "Avancerat"

#: gtkusersetup.py:224
msgid "Password is too short. It has to be at least 5 characters long."
msgstr "Lösenordet är för kort. Det måste vara minst 5 tecken långt."

#: gtkusersetup.py:227
msgid "Password cannot include the backslash character."
msgstr "Lösenordet kan inte innehålla bakstreck."

#: gtkusersetup.py:233
msgid "Passwords don't match"
msgstr "Lösenorden matchar inte"

#: gtkusersetup.py:242
msgid "Username cannot be empty."
msgstr "Användarnamn kan inte vara tomt."

#: gtkusersetup.py:245
msgid "Username cannot exceed 31 characters"
msgstr "Användarnamnet kan inte vara mer än 31 tecken"

#: gtkusersetup.py:248
msgid "Username cannot include the space character."
msgstr "Användarnamn kan inte innehålla mellanslag."

#: gtkusersetup.py:251
msgid "Username cannot start with a number."
msgstr "Användarnamn kan inte börja med en siffra."

#: gtkusersetup.py:254
msgid "Username cannot start with an underscore."
msgstr "Användarnamn kan inte börja med understreck."

#: gtkusersetup.py:257
msgid "Username cannot start with a dash."
msgstr "Användarnamn kan inte börja med streck."

#: gtkusersetup.py:263
msgid ""
"Username can consist solely of latin lower case letters, numbers, dashes and"
" underscores."
msgstr "Användarnamn kan endast innehålla liten bokstav, siffror, streck och understreck."

#: gtkusersetup.py:269
#, python-brace-format
msgid "Username \"{0}\" already exists."
msgstr "Användarnamnet \"{0}\" finns redan."

#: gtkusersetup.py:280
msgid "Group name cannot be empty."
msgstr "Grupp namn kan inte vara tomt."

#: gtkusersetup.py:283
msgid "Group name cannot exceed 31 characters"
msgstr "Grupp namn kan inte vara längre än 31 tecken"

#: gtkusersetup.py:286
msgid "Group name cannot include the space character."
msgstr "Grupp namn kan inte innehålla mellanslag."

#: gtkusersetup.py:289
msgid "Group name cannot start with a number."
msgstr "Grupp namn kan inte börja med en siffra."

#: gtkusersetup.py:292
msgid "Group name cannot start with an underscore."
msgstr "Grupp namn kan inte börja med ett understreck."

#: gtkusersetup.py:295
msgid "Group name cannot start with a dash."
msgstr "Grupp namn kan inte börja med ett streck."

#: gtkusersetup.py:301
msgid ""
"Group name can consist solely of latin lower case letters, numbers, dashes "
"and underscores."
msgstr "Grupp namn kan endast innehålla liten bokstav, siffror, streck och understreck."

#: gtkusersetup.py:307
#, python-brace-format
msgid "Group name \"{0}\" already exists."
msgstr "Grupp namn \"{0}\" finns redan."

#: gtkusersetup.py:318
msgid "Shell does not exist."
msgstr "Skal finns inte."

#: gtkusersetup.py:328
#, python-brace-format
msgid "UID {0} is assigned to another user."
msgstr "UID {0} tillhör en annan användare."

#: gtkusersetup.py:338
#, python-brace-format
msgid "GID {0} is assigned to another group."
msgstr "GID {0} tillhör en annan grupp."

#: gtkusersetup.py:391
#, python-brace-format
msgid "User account properties for \"{0}\""
msgstr "Egenskaper för användar konto \"{0}\""

#: gtkusersetup.py:398
msgid "<b>Set new password</b>"
msgstr "<b>Sätt nytt lösenord</b>"

#: gtkusersetup.py:456
msgid "Add new user"
msgstr "Lägg till ny användare"

#: gtkusersetup.py:463
msgid "<b>Set password</b>"
msgstr "<b>Sätt lösenord</b>"

#: gtkusersetup.py:514
#, python-brace-format
msgid "Delete user {0}?"
msgstr "Ta bort användaren {0}?"

#: gtkusersetup.py:516
#, python-brace-format
msgid ""
"You are about to remove user account \"{0}\" from your system. Do you really"
" want to continue?"
msgstr "Du håller på att ta bort användar konto \"{0}\" från systemet. Vill du verkligen fortsätta?"

#: gtkusersetup.py:518
#, python-brace-format
msgid "Completely remove user's home directory ({0}) with all its contents"
msgstr "Ta bort användarens hemkatalog ({0}) med allt innehåll"

#: gtkusersetup.py:775
#, python-brace-format
msgid "Group \"{0}\" properties"
msgstr "Grupp \"{0}\" egenskaper"

#: gtkusersetup.py:794
msgid "Add new group"
msgstr "Lägg till grupp"

#: gtkusersetup.py:820
#, python-brace-format
msgid ""
"You are about to delete the \"{0}\" group from your system. Are you sure you"
" want to continue?"
msgstr "Du håller på att ta bort gruppen \"{0}\" från ditt system. Är du säker på att du vill fortsätta?"

#: gtkusersetup.py:824
#, python-brace-format
msgid ""
"Group \"{0}\" cannot be deleted as it is the main group for user \"{1}\". If"
" you want to delete group \"{0}\", you have to set the main group for user "
"\"{1}\" to another group first."
msgstr "Grupp \"{0}\" kan inte raderas eftersom det är huvudgruppen för användare \"{1}\". Om du vill ta bort gruppen \"{0}\", måste du ändra huvudgruppen för användare \"{1}\" till en annan grupp först."

